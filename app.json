{
  "expo": {
    "name": "InvestFlipkart",
    "slug": "InvestFlipkart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/flipcart.png",
    "scheme": "investflipkart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "update": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "versionCode": 1,
      "permissions": [],
      "package": "invest.flipkart.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/flipcart.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/flipcart.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFD700"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "jsc",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "98025ca3-644d-404b-b591-29372ffd89b2"
      }
    },
    "owner": "invest_flipkart",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/98025ca3-644d-404b-b591-29372ffd89b2"
    }
  }
}
